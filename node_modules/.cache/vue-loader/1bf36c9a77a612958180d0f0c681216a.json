iv class=\"honor\"></div>\r\n            </div>\r\n        </div>\r\n\r\n\r\n        <div class=\"menu\">\r\n            <router-link tag=\"div\" class=\"glance\" to=\"\">浏览</router-link>\r\n            <router-link tag=\"div\" class=\"self_article\" to=\"\">文章</router-link>\r\n            <router-link tag=\"div\" class=\"question\" to=\"\">问题</router-link>\r\n            <router-link tag=\"div\" class=\"answer\" to=\"\">回答</router-link>\r\n            <router-link tag=\"div\" class=\"focus\" to=\"\">关注</router-link>\r\n            <router-link tag=\"div\" class=\"collect\" to=\"\">收藏</router-link>\r\n        </div>\r\n\r\n        <div class=\"content\">\r\n            <router-view class=\"\"></router-view>\r\n        </div>\r\n        \r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport CropperImage from \"@/components/CropperImage\";\r\n\r\nexport default {\r\n    name: \"user_info\",\r\n    components: {CropperImage},\r\n    data() {\r\n        return {\r\n            cover_url: require('@/assets/photo/img003.jpg'),\r\n            avatar_url: require('@/assets/photo/img004.jpg'),\r\n            username: \"前面有树\",\r\n            occupation: \"船舶与海洋工程\",\r\n            formValidate: {\r\n                mainImage: '',\r\n      },\r\n      ruleValidate: {\r\n        mainImage: [\r\n          {required: true, message: '请上传封面', trigger: 'blur'}\r\n        ],\r\n      },\r\n      //裁切图片参数\r\n      cropperModel:false,\r\n      cropperName:'',\r\n      imgName: '',\r\n      imgVisible: false\r\n        }\r\n    },\r\n    methods: {\r\n        //封面设置\r\n        uploadPicture(name){\r\n        this.cropperName = name;\r\n        this.cropperModel = true;\r\n        },\r\n        //图片上传成功后\r\n        handleUploadSuccess (data){\r\n            console.log(data)\r\n            switch(data.name){\r\n                case 'flagImg':\r\n                this.formValidate.mainImage = 'http://ydfblog.cn/dfs/'+data.url;\r\n                console.log('最终输出'+data.name)\r\n    